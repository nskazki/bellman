{
  "name": "bellman",
  "version": "1.0.8",
  "description": "pretty console logger",
  "main": "src-build/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build-src": "babel src --retain-lines --source-maps inline --out-dir src-build",
    "build-test-project": "babel test-project --retain-lines --source-maps inline --out-dir test-project-build",
    "build": "npm run build-src && npm run build-test-project",
    "test-project": "node test-project-build",
    "test": "npm run build && npm run test-project"
  },
  "keywords": [
    "logger",
    "console"
  ],
  "author": "nskazki@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nskazki/bellman.git"
  },
  "bugs": {
    "url": "https://github.com/nskazki/bellman/issues"
  },
  "homepage": "https://github.com/nskazki/bellman#readme",
  "dependencies": {
    "chalk": "^1.1.1",
    "lodash": "^3.10.1",
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "babel": "^5.8.23"
  }
}
